  "dependencies": {},
  "devDependencies": {
    "eslint": "^4.5.0",
    "eslint-config-wordpress": "^2.0.0",
    "stylelint-config-wordpress": "^12.0.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "phplint": "find . -name '*.php' -not -path './node-modules/*' -not -path './vendor/*' -exec php -lf '{}' ';'",
    "stylelint": "node node_modules/stylelint/bin/stylelint.js --config ./.stylelint public/css/*.css admin/css/*.css",
    "eslint": "node node_modules/eslint/bin/eslint.js --config ./.eslintrc.js 'public/js/*.js' 'admin/js/*.js' --quiet",
    "phpcs": "php vendor/squizlabs/php_codesniffer/bin/phpcs --standard=./.phpcs.xml",
    "phpcbf": "php vendor/squizlabs/php_codesniffer/bin/phpcbf --standard=./.phpcs.xml"
  },
